SRC=@srcdir@
TESTS=clean t1
OUT=t1.one.ps.expected
all: ${TESTS}

clean:
	rm  -f *.out
t1:
	../foomatic-rip -v --ppd=./ppd1 <${SRC}/one.ps >t1.one.ps.out
	diff t1.one.ps.out ${SRC}/t1.one.ps.expected || \
	echo "WARNING: Output does not match reference file!"
#	../foomatic-rip -v --ppd=./ppd1 <${SRC}/one.pcl >t1.one.pcl.out
#	diff t1.one.pcl.out ${SRC}/t1.one.pcl.expected || \
#	echo "WARNING: Output does not match reference file!"
	../foomatic-rip -v --ppd=./ppd1 <${SRC}/one.pjl >t1.one.pjl.out
	diff t1.one.pjl.out ${SRC}/t1.one.pjl.expected || \
	echo "WARNING: Output does not match reference file!"
	env PRINTCAP_ENTRY="lp:ppd=./ppd1" ../foomatic-rip -v <${SRC}/one.pjl >t1.one.pjl.out
	diff t1.one.pjl.out ${SRC}/t1.one.pjl.expected || \
	echo "WARNING: Output does not match reference file!"
